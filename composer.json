{
  "name": "fakeller/archinspec",
  "type": "project",
  "description": "Static analysis tool for PHP architectures. Define how your architecture should look like and let archinspec validate it.",
  "homepage": "https://github.com/FaKeller/archinspec",
  "license": "MIT",
  "keywords": [
    "architecture",
    "dependency",
    "static",
    "analysis"
  ],
  "authors": [
    {
      "name": "Fabian Keller",
      "email": "hello@fabian-keller.de"
    }
  ],
  "support": {
    "issues": "https://github.com/FaKeller/archinspec/issues",
    "source": "https://github.com/FaKeller/archinspec"
  },
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "require": {
    "php": ">=5.5",
    "mamuz/php-dependency-analysis": "~1.0",
    "symfony/yaml": "~2.5|~3.0",
    "symfony/config": "~2.5|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "bin": [
    "bin/archinspec"
  ]
}
